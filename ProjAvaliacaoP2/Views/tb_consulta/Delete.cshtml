@model ProjAvaliacaoP2.tb_consulta

@{
    ViewBag.Title = "Delete";
}

<h2>Deletar</h2>

<h3>Tem certeza que deseja deletar o registro?</h3>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Data da Consulta
        </dt>

        <dd>
            @Html.DisplayFor(model => model.data_consulta)
        </dd>

        <dt>
            Observação
        </dt>

        <dd>
            @Html.DisplayFor(model => model.observacao)
        </dd>

        <dt>
            Clínica
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tb_clinica.nome)
        </dd>

        <dt>
            Médico
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tb_medico.nome)
        </dd>

        <dt>
            Paciente
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tb_paciente.nome)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Deletar" class="btn btn-default" /> |
            @Html.ActionLink("Voltar", "Index")
        </div>
    }
</div>
